{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Expo App",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/expo/bin/cli.js",
      "args": ["start", "--web"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Run Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest.js",
      "args": ["--watch"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}