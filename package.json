{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "build": "expo export --platform web --output-dir dist",
    "build-web": "expo export --platform web --output-dir dist",
    "build-android": "expo export --platform android --output-dir dist",
    "build-ios": "expo export --platform ios --output-dir dist", 
    "build-all": "expo export --output-dir dist",
    "netlify-build": "npx expo export --platform web --output-dir dist",
    "build-safe": "npx expo export --platform web --output-dir dist || (mkdir -p dist && cp public/index.html dist/index.html)",
    "compile": "npx tsc --noEmit",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:unit": "node __tests__/test-runner.js",
    "test:quick": "node __tests__/test-runner.js",
    "test:auto": "node run-automated-tests.js",
    "test:complete": "node test-master.js",
    "test:flow": "node test-complete-flow.js",
    "test:ci": "node test-master.js",
    "test:master": "node test-master.js",
    "verify": "npm run test:auto",
    "pre-deploy": "node verify-pre-deploy.js",
    "check": "node verify-pre-deploy.js",
    "doctor": "expo doctor"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/native": "^7.1.6",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.83.0",

    "@ungap/structured-clone": "^1.3.0",
    "expo": "^53.0.4",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.4",
    "expo-font": "~13.3.0",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-location": "~18.1.4",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.0.3",
    "expo-splash-screen": "~0.30.7",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
